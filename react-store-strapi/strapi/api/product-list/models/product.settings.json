{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "products_data",
  "info": {
    "name": "product list"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "description": {
      "type": "richtext"
    },
    "featured": {
      "type": "boolean"
    },
    "free_shipping": {
      "type": "boolean"
    },
    "extra_field": {
      "type": "string"
    },
    "category": {
      "type": "string"
    }
  }
}
